    "@types/jest": "^26.0.23",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
